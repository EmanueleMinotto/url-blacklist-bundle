{
    "authors": [
        {
            "email": "minottoemanuele@gmail.com",
            "homepage": "https://github.com/EmanueleMinotto",
            "name": "Emanuele Minotto",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "EmanueleMinotto\\UrlBlacklistBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmanueleMinotto\\UrlBlacklistBundle\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Symfony Bundle for URLs blacklisting",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "homepage": "https://github.com/EmanueleMinotto/url-blacklist-bundle",
    "keywords": [
        "symfony",
        "bundle",
        "blacklist",
        "url"
    ],
    "license": "MIT",
    "name": "emanueleminotto/url-blacklist-bundle",
    "require": {
        "php": "^5.6 || ^7.0",
        "jeremykendall/php-domain-parser": "^3.0",
        "symfony/symfony": "^2.8 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^1.1",
        "phpunit/phpunit": "^5.4.3"
    },
    "scripts": {
        "test": [
            "php-cs-fixer --ansi -vv fix",
            "phpunit --colors=always"
        ]
    },
    "type": "library"
}
